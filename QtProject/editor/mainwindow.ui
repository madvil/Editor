	color: rgb(185, 185, 185);
	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(120, 120, 120, 255), stop:0.158192 rgba(120, 120, 120, 255), stop:0.619586 rgba(80, 80, 80, 255), stop:1 rgba(80, 80, 80, 255));
     border: 1px solid #333333;
     min-width: 8ex;
     padding: 3px;
 border-bottom: 0px;
 }

 QTabBar::tab:selected, QTabBar::tab:hover {
    
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(90, 90, 90, 255), stop:0.494318 rgba(90, 90, 90, 255), stop:0.5 rgba(71, 71, 71, 255), stop:1 rgba(71, 71, 71, 255));
color:rgb(255,255,255);
 border-bottom: 0px;
 }

 QTabBar::tab:selected { 
     border-color: #000000;
 }</string>
