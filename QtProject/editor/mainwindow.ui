/* QMenuBar */

QMenuBar {
border-bottom: 1px solid #000000;
background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(130, 130, 130, 255), stop:1 rgba(65, 65, 65, 255));
}

QMenuBar::item {
     spacing: 4px; 
     padding: 4px;
 	padding-left: 18px;
     background: transparent;
     border-radius: 4px;
 }

/* QMenu */

QMenu {
    border: 1px solid rgb(75, 75, 75);
 }

QMenu::item:selected {
     background-color:  rgb(75, 75, 75);
 }

/* QSplitter */

 QSplitter::handle:horizontal {
     width: 3px;
 }

 QSplitter::handle:vertical {
     height: 3px;
 }

 QSplitter::handle:pressed {
	background: rgb(204, 204, 204);
 }

/* QScrollBar */

QScrollBar:vertical {
      border: 1px solid rgb(95, 95, 95);
border-radius: 4px;
      background: rgb(65, 65, 65);
      min-width: 15px;
      margin: 22px 0 22px 0;
  }
  QScrollBar::handle:vertical {
      background: rgb(50, 50, 50);
	  border: 1px solid rgb(85, 85, 85);
      border-radius: 4px;
      min-height: 20px;
  }
  QScrollBar::add-line:vertical {
      border: 1px solid rgb(85, 85, 85);
      background: rgb(50, 50, 50);
	  border-radius: 4px;
      height: 20px;
      subcontrol-position: bottom;
      subcontrol-origin: margin;
  }

  QScrollBar::sub-line:vertical {
      border: 1px solid rgb(85, 85, 85);
      background: rgb(50, 50, 50);
border-radius: 4px;
      height: 20px;
      subcontrol-position: top;
      subcontrol-origin: margin;
  }
  QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
      border: none;
      width: 2px;
      height: 4px;
      background: white;
  }

  QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
      background:  rgb(65, 65, 65);
  }

/* PushButton */

QPushButton {
border: 1px solid #000000;
     border-radius: 4px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(90, 90, 90, 255), stop:0.494318 rgba(90, 90, 90, 255), stop:0.5 rgba(71, 71, 71, 255), stop:1 rgba(71, 71, 71, 255));
     min-width: 20px;
 }

 QPushButton:pressed {    
	 background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(100, 100, 100, 255), stop:0.494318 rgba(100, 100, 100, 255), stop:0.5 rgba(81, 81, 81, 255), stop:1 rgba(81, 81, 81, 255));
padding-top: 2px;
padding-left: 2px;
 }

/* QHeaderView */

QHeaderView::section {     
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(90, 90, 90, 255), stop:0.494318 rgba(90, 90, 90, 255), stop:0.5 rgba(71, 71, 71, 255), stop:1 rgba(71, 71, 71, 255));
padding: 3px;
     padding-left: 6px;
     border: 0px solid #000000;
border-top: 1px solid #000000;
 }

/* TreeWidget */

QTreeView {
 alternate-background-color: rgb(65, 65, 65);
 border: 0px solid #000000;
}

 QTreeView::item {
      border: 0px solid #000000;
     border-top-color: transparent;
     border-bottom-color: transparent;
 }

QTreeView::item:selected:active{
     background: rgb(65, 65, 65);
 }

 QTreeView::item:selected:!active {
     background: rgb(65, 65, 65);
 }
QTreeView::branch {
         background:transparent;
 }

 QTreeView::branch:has-siblings:!adjoins-item {
         background:transparent;
 }

 QTreeView::branch:has-siblings:adjoins-item {
         background:transparent;
 }

 QTreeView::branch:!has-children:!has-siblings:adjoins-item {
         background:transparent;
 }

 QTreeView::branch:closed:has-children:has-siblings {
       background:transparent;
 }

 QTreeView::branch:has-children:!has-siblings:closed {
         background:transparent;
 }

 QTreeView::branch:open:has-children:has-siblings {
       background:transparent;
 }

 QTreeView::branch:open:has-children:!has-siblings {
        background:transparent;
 }

QTreeView::branch:has-children:!has-siblings:closed,
 QTreeView::branch:closed:has-children:has-siblings {
         border-image: none;
image: url(:/media/media/stylesheet-branch-closed.png);
 }

 QTreeView::branch:open:has-children:!has-siblings,
 QTreeView::branch:open:has-children:has-siblings  {
         border-image: none;
	image: url(:/media/media/stylesheet-branch-open.png);      
 }

/* TabWidget */

QTabWidget::pane { 
     position: absolute;
     border: 1px solid #000000;
 }

QTabWidget::tab-bar {
     alignment: left;
 }

